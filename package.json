{
  "name": "disaster-management-pwa",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "dev:pwa": "PWA_TESTING=true next dev",
    "build": "next build",
    "build:test": "cross-env NODE_ENV=test next build",
    "build:validated": "npm run validate:schema && next build && npm run test:smoke",
    "start": "next start",
    "lint": "next lint",
    "type-check": "tsc --noEmit",
    "test": "playwright test",
    "test:headed": "playwright test --headed",
    "test:e2e": "playwright test tests/e2e/",
    "test:e2e:headed": "playwright test tests/e2e/ --headed",
    "test:pwa": "PWA_TESTING=true playwright test tests/e2e/pwa.test.ts",
    "test:pwa-dev": "cross-env PWA_TESTING=true npm run dev & wait-on http://localhost:3000 && npm run test:pwa",
    "test:unit": "jest",
    "test:unit:watch": "jest --watch",
    "test:unit:coverage": "jest --coverage",
    "test:integration": "jest --testMatch='<rootDir>/tests/integration/**/*.test.{js,jsx,ts,tsx}'",
    "test:response-planning": "bash scripts/test-response-planning.sh",
    "test:response-planning:unit": "jest tests/unit/services/response-planning.test.ts tests/unit/components/ResponsePlanningForm.test.tsx",
    "test:response-planning:integration": "jest tests/integration/response-planning/",
    "test:response-planning:e2e": "playwright test tests/e2e/response-planning-workflow.test.ts",
    "test:smoke": "playwright test --config=playwright-smoke.config.ts",
    "test:smoke:headed": "playwright test --config=playwright-smoke.config.ts --headed",
    "test:smoke:story-5.1": "playwright test tests/smoke/story-5.1-donor-registration.test.ts --config=playwright-smoke.config.ts",
    "validate:pre-story": "bash scripts/validate-pre-story.sh",
    "validate:schema": "node scripts/validate-schema-usage.js",
    "verify:baseline": "bash scripts/testing/regression-prevention/verify-baseline.sh",
    "regression:create": "bash scripts/testing/regression-prevention/create-regression-tests.sh",
    "regression:analyze": "node scripts/testing/regression-prevention/feature-impact-analyzer.js",
    "dev:validate": "node scripts/testing/validation/development-validator.js",
    "living-tests": "node scripts/testing/living-tests/living-tests-cli-simple.js",
    "claude:dev": "set MCP_CONFIG=.mcp.dev.json && claude-code"
  },
  "dependencies": {
    "@hookform/resolvers": "^3.9.1",
    "@next-auth/prisma-adapter": "^1.0.7",
    "@prisma/client": "^6.16.3",
    "@radix-ui/react-alert-dialog": "^1.1.15",
    "@radix-ui/react-avatar": "^1.1.10",
    "@radix-ui/react-checkbox": "^1.3.3",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-popover": "^1.1.15",
    "@radix-ui/react-progress": "^1.1.7",
    "@radix-ui/react-scroll-area": "^1.2.10",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-separator": "^1.1.7",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-switch": "^1.2.6",
    "@radix-ui/react-tabs": "^1.1.13",
    "@radix-ui/react-toast": "^1.2.15",
    "@radix-ui/react-tooltip": "^1.2.8",
    "@tanstack/react-query": "^5.90.2",
    "@types/leaflet": "^1.9.8",
    "autoprefixer": "10.4.19",
    "bcryptjs": "^3.0.2",
    "chart.js": "^4.5.1",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cmdk": "^1.1.1",
    "command": "^1.1.1",
    "commander": "^14.0.2",
    "date-fns": "^4.1.0",
    "dexie": "4.0.8",
    "jsonwebtoken": "^9.0.2",
    "leaflet": "^1.9.4",
    "lucide-react": "^0.544.0",
    "mini-css-extract-plugin": "^2.9.4",
    "next": "14.2.5",
    "next-auth": "^4.24.11",
    "next-pwa": "5.6.0",
    "postcss": "8.4.39",
    "prisma": "^6.16.3",
    "react": "18.3.1",
    "react-chartjs-2": "^5.3.1",
    "react-day-picker": "^9.11.2",
    "react-dom": "18.3.1",
    "react-hook-form": "7.51.5",
    "react-leaflet": "^4.2.1",
    "react-leaflet-cluster": "^4.0.0",
    "react-leaflet-markercluster": "^4.1.0",
    "sonner": "^2.0.7",
    "tailwind-merge": "^3.3.1",
    "tailwindcss": "3.4.6",
    "typescript": "^5.5.4",
    "uuid": "^13.0.0",
    "zod": "3.23.8",
    "zustand": "4.5.5"
  },
  "devDependencies": {
    "@babel/core": "^7.28.4",
    "@babel/preset-env": "^7.28.3",
    "@playwright/test": "^1.55.1",
    "@tanstack/react-query-devtools": "^5.90.2",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/bcryptjs": "^3.0.0",
    "@types/jest": "^29.5.12",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@types/supertest": "^6.0.3",
    "@types/uuid": "^11.0.0",
    "babel-loader": "^10.0.0",
    "cross-env": "^7.0.3",
    "eslint": "^8",
    "eslint-config-next": "14.2.5",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "node-mocks-http": "^1.17.2",
    "supertest": "^7.1.4",
    "ts-jest": "^29.1.1",
    "wait-on": "^8.0.1"
  }
}
